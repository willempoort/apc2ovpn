clientdev tunproto {protocol}remote {server_address} {server_port}verify-x509-name "{server_dn}"route remote_host 255.255.255.255 net_gatewayresolv-retry infinitenobindpersist-keypersist-tunauth-user-pass /etc/openvpn/{outputfile}.authcipher {encryption_algorithm}auth {authentication_algorithm}comp-lzo route-delay 4verb 3reneg-sec 0<ca>{ca_cert}</ca><cert>{certificate}</cert><key>{key}</key>